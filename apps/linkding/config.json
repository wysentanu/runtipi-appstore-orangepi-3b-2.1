  "form_fields": [
    {
      "type": "text",
      "label": "Host Data DIR",
      "required": true,
      "hint": "Host data DIR",
      "placeholder": "${APP_DATA_DIR}/data",
      "default": "${APP_DATA_DIR}/data",
      "env_variable": "LD_HOST_DATA_DIR"
    },
    {
      "type": "text",
      "label": "Superuser Name",
      "required": true,
      "hint": "Username of the initial superuser to create",
      "placeholder": "admin",
      "default": "admin",
      "env_variable": "LD_SUPERUSER_NAME"
    },
    {
      "type": "text",
      "label": "Superuser Password",
      "required": true,
      "hint": "Password for the initial superuser",
      "placeholder": "admin",
      "default": "admin",
      "env_variable": "LD_SUPERUSER_PASSWORD"
    },
    {
      "type": "boolean",
      "label": "Disable Background Tasks",
      "required": false,
      "hint": "Option to disable background tasks",
      "placeholder": "False",
      "default": "False",
      "env_variable": "LD_DISABLE_BACKGROUND_TASKS"
    },
    {
      "type": "boolean",
      "label": "Disable URL Validation",
      "required": false,
      "hint": "Option to disable URL validation for bookmarks completely",
      "placeholder": "False",
      "default": "False",
      "env_variable": "LD_DISABLE_URL_VALIDATION"
    }
  ]
}
